type Student{
    cin: String!
    nombre: String!
    email: String!
}

type Query{
    allStudents: [Student]
    searchStudById(id: String!): Student

}
type Mutation{
	addStud(cin: String,nombre: String,email: String): [Student]
	delete(id: String): [Student]
	update(cin: String,nombre: String,email :String): [Student]
}

 
schema {
    query: Query
    mutation: Mutation
    }